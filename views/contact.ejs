<!-- Start of contact form -->
<div class="container">
    <div class="row d-flex justify-content-center">
        <div class="col-5 text-center contactform p-3">
            <form action="/submitContact" method="post">
                <h3 class="mb-4">Contact Me</h3>
                <input type="text" class="form-control mb-4" name="fname" aria-describedby="firstname" placeholder="First Name"> 
                <input type="text" class="form-control mb-4" name="lname" aria-describedby="lastname" placeholder="Last Name"> 
                <input type="email" class="form-control mb-4" name="email" aria-describedby="emailHelp" placeholder="Email"> 
                <label for="selection" class="form-label">Reason for contact:</label>
                <select class="form-select" aria-label="Default select example" name="reason">
                    <option selected></option>
                    <option  value="Bug">Bugs</option>
                    <option value="Opportunity">Opportunity</option>
                    <option value="Connect">Connect</option>
                </select>
                <br>
                <textarea class="form-control mb-4" name="comment" rows="3" placeholder="Comment"></textarea>
                <div class="d-grid">
                    <button type="submit" class="btn btn-primary">Submit</button>
                </div>
            </form>
        </div>
        <div class="col-5">
            <!--Google map-->
            <div id="map-container-google-1" class="z-depth-1-half map-container" style="height: 100%">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1739.637154886563!2d-98.52584614164424!3d29.303628195539414!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x865c57620df39603%3A0x5512fcd4af8dfd1a!2sOne%20University%20Way%2C%20San%20Antonio%2C%20TX%2078224!5e0!3m2!1sen!2sus!4v1658293291381!5m2!1sen!2sus" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade" frameborder="0"
                style="width: 100%; height: 100%;" allowfullscreen></iframe>
            </div>
        </div>
    </div>
</div>

